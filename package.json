{
    "name": "fractured-bootlegger",
    "version": "1.0.0",
    "description": "Bootlegged Installer for Fractured Space",
    "repository": {
        "type": "git",
        "url": "https://github.com/rukqoa/fractured-bootlegger.git"
    },
    "main": "main.js",
    "scripts": {
        "start": "electron .",
